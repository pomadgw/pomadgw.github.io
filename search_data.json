---
layout: null
---
{
  {% for post in site.posts %}

    "{{ post.url | slugify }}": {
      "title": "{{ post.title | xml_escape }}",
      "url": " {{ post.url | xml_escape }}",
      "categories": "{% for c in post.categories %}{{ c | xml_escape }} {% endfor %}",
      "text": "{{ post.content | strip_html | strip_newlines }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
}